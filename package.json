{
  "name": "edge-logs-server",
  "version": "0.1.0",
  "private": false,
  "description": "Logging Tool",
  "repository": "https://github.com/EdgeApp/edge-logs-server",
  "license": "MIT",
  "author": "Airbitz, Inc.",
  "scripts": {
    "build": "sucrase -q -t typescript,imports,jsx -d ./lib ./src",
    "clean": "rimraf dist lib",
    "client": "parcel serve src/client/index.html",
    "configure": "configure",
    "dev": "npm-run-all -p client start",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "precommit": "lint-staged && npm-run-all types prepare",
    "prepare": "husky install && npm-run-all clean configure build",
    "start": "node -r sucrase/register src/indexApi.ts",
    "types": "tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "dependencies": {
    "cleaner-config": "^0.1.4",
    "cleaners": "^0.2.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "edge-server-tools": "^0.1.0",
    "express": "^4.17.1",
    "luxon": "^1.27.0",
    "nano": "^8.2.2",
    "prop-types": ">= 15.5.10 < 16.0.0",
    "react-data-table-component": "^6.11.6",
    "react-datepicker": "^3.3.0",
    "react-json-view": "^1.19.1",
    "react-loader-spinner": "^3.1.14",
    "react-router-dom": "^5.2.0",
    "react-timezone-select": "^1.0.3",
    "styled-components": "^5.2.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-standard-kit": "^0.15.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": ">=9.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^2.2.1",
    "react": ">= 16.3.0",
    "react-dom": "^16.0.0 || ^15.5.4",
    "rimraf": "^3.0.2",
    "sucrase": "^3.12.1",
    "typescript": "^4.2.3"
  }
}
